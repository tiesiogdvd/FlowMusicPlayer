<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="360"
                android:centerColor="#E1E1E1"
                android:endColor="#DDDCDC"
                android:gradientRadius="360"
                android:startColor="#818181"
                android:type="radial" />
            <size android:height="170dp"
                android:width="170dp"/>
        </shape>
    </item>
    <item android:top="2dp" android:bottom="2dp" android:right="2dp" android:left="2dp">
        <shape android:shape="oval">
            <size android:width="140dp"
                android:height="140dp"/>
            <solid android:color="#72858585"/>
            <solid android:color="#83FFFFFF"/>
        </shape>
    </item>
</layer-list>