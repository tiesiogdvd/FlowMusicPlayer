<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">


<style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
    <item name="android:windowBackground">#CD0000</item>
    <item name="android:textColor">@color/text</item>
    <item name="drawableTint">@color/drawable_menu</item>
    <item name="neumorph_shadowColorDark">@color/neumorphDark</item>
    <item name="neumorph_shadowColorLight">@color/neumorphLight</item>
    <item name="neumorph_shadowElevation">2dp</item>
    <item name="android:windowContentTransitions">true</item>
    <item name="android:windowIsFloating">false</item>
</style>



<style name="dark">
    <item name="android:textColor">#FFFFFF</item>
    <item name="android:tint">#FFFFFF</item>
</style>

<style name="light">
    <item name="android:textColor">#000000</item>
    <item name="android:tint">#000000</item>
</style>

<style name="white">
    <item name="android:textColor">#FFFFFF</item>
    <item name="android:tint">#FFFFFF</item>
</style>

<style name="black">
    <item name="android:textColor">#000000</item>
    <item name="android:tint">#000000</item>
</style>

<style name="transparent">
    <item name="android:textColor">#000000</item>
    <item name="android:tint">#2A95C588</item>
</style>

<style name= "round">
    <item name="cornerSize">50%</item>
</style>

    <style name="ImageTransition" parent="android:style/Animation">
        <item name="android:windowEnterTransition">@transition/image_transition</item>
        <item name="android:windowExitTransition">@transition/image_transition</item>
    </style>
</resources>